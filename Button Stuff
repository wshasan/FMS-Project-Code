let color = 220;
let button1 = null; 

function setup() {
  createCanvas(400, 400);
  background(color);
  
  button1 = createButton("test");
  button1.position(width / 2, height / 2);
  button1.size(width / 8, height / 16);
  button1.mousePressed(changeColorCanvas);
  //button stuff should be in setup() method
}

function draw() {
  
}

function changeColorCanvas() {
  color = random(255);
  background(color);
}
